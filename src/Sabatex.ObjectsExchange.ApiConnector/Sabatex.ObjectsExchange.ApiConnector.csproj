<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks Condition="'$(LibraryFrameworks)'==''">net6.0;net35;netstandard2.0;netstandard2.1;net7.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(LibraryFrameworks)'!=''">$(LibraryFrameworks)</TargetFrameworks>
		<LangVersion>9.0</LangVersion>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NeutralLanguage>en-US</NeutralLanguage>
		<Authors>Serhiy Lakas</Authors>
		<Copyright>2022, Serhiy Lakas</Copyright>
		<Product>Models for Exchange APi</Product>
		<Description>API Connector for ObjectExchange service</Description>
		<Company>sabatex on git</Company>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageId>Sabatex.ObjectsExchange.ApiConnector</PackageId>
		<PackageTags>#sabatex #Exchange #API </PackageTags>
		<Version>1.0.0-alpha2</Version>
		<AnalysisLevel>latest-minimum</AnalysisLevel>
	</PropertyGroup>

	<ItemGroup>
		<Folder Include="BASApi\" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)'=='net6.0'">
	  <ProjectReference Include="..\..\Sabatex.ObjectsExchange.ApiConnector.HttpClient\Sabatex.ObjectsExchange.ApiConnector.HttpClient.csproj" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)'=='net7.0'">
		<ProjectReference Include="..\..\Sabatex.ObjectsExchange.ApiConnector.HttpClient\Sabatex.ObjectsExchange.ApiConnector.HttpClient.csproj" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)'=='net35'">
		
		<ProjectReference Include="..\..\Sabatex.ObjectsExchange.ApiConnector.WebClient\Sabatex.ObjectsExchange.ApiConnector.WebClient.csproj" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)'=='netstandard2.0'">
		<ProjectReference Include="..\..\Sabatex.ObjectsExchange.ApiConnector.WebClient\Sabatex.ObjectsExchange.ApiConnector.WebClient.csproj" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)'=='netstandard2.1'">
		<ProjectReference Include="..\..\Sabatex.ObjectsExchange.ApiConnector.WebClient\Sabatex.ObjectsExchange.ApiConnector.WebClient.csproj" />
	</ItemGroup>



</Project>
