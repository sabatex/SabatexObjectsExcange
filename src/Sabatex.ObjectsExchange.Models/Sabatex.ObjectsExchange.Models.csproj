<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFrameworks Condition="'$(LibraryFrameworks)'==''">net3.5;netstandard2.0;netstandard2.1;net6.0;net7.0</TargetFrameworks>
	<TargetFrameworks Condition="'$(LibraryFrameworks)'!=''">$(LibraryFrameworks)</TargetFrameworks>
	<LangVersion>9.0</LangVersion>
	<Nullable>enable</Nullable>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>  
	<NeutralLanguage>en-US</NeutralLanguage>
	<Authors>Serhiy Lakas</Authors>
	<Copyright>2022, Serhiy Lakas</Copyright>
	<Product>Models for Exchange APi</Product>
	<Description>Models for Exchange APi</Description>
	<Company>sabatex on git</Company>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageId>Sabatex.ObjectsExchange.Models</PackageId>
	<PackageTags>#Exchange #API </PackageTags>
	<Version>1.0.0-rc10</Version>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="Sabatex.Core" Version="1.1.1" />
  </ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net3.5'">
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.1'">
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
